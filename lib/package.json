{
  "name": "@coderatparadise/showrunner-time",
  "version": "0.1.0",
  "description": "Time library for showrunner modules",
  "packageManager": "yarn@3.2.1",
  "type": "module",
  "files": [
    "lib/**"
  ],
  "exports": {
    ".": {
      "types": "./lib/types/index.js",
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./*/.js": {
      "types": "./lib/types/*.js",
      "import": "./lib/esm/*.js",
      "require": "./lib/cjs/*.js"
    },
    "./codec": {
      "types": "./lib/types/codec/index.js",
      "import": "./lib/esm/codec/index.js",
      "require": "./lib/cjs/codec/index.js"
    },
    "./codec/*.js": {
      "types": "./lib/types/codec/*.js",
      "import": "./lib/esm/codec/*.js",
      "require": "./lib/cjs/codec/*.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && tsc -p tsconfig-declarations.json",
    "start": "node lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CoderAtParadies/showrunner-time/lib"
  },
  "author": "CoderAtParadise",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CoderAtParadies/showrunner-time/issues"
  },
  "homepage": "https://github.com/CoderAtParadies/showrunner-time/lib#readme",
  "dependencies": {
    "@coderatparadise/showrunner-network": "^1.0.1",
    "@trpc/server": "^10.0.0-alpha.38",
    "typescript": "^4.7.4",
    "zod": "^3.17.9"
  },
  "devDependencies": {
    "@types/eslint": "^8",
    "@types/node": "^18.0.0",
    "@types/prettier": "^2",
    "@yarnpkg/sdks": "^3.0.0-rc.9",
    "eslint": "^8.18.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-standard": "^1.0.2",
    "prettier": "^2.7.1"
  }
}
