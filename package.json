{
  "name": "@coderatparadise/showrunner-time",
  "version": "0.1.1",
  "description": "Time library for showrunner modules",
  "packageManager": "yarn@3.2.1",
  "type": "module",
  "files": [
    "lib/**"
  ],
  "exports": {
    ".": {
      "types": "./lib/types/index.js",
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./*/.js": {
      "types": "./lib/types/*.js",
      "import": "./lib/esm/*.js",
      "require": "./lib/cjs/*.js"
    },
    "./codec": {
      "types": "./lib/types/codec/index.js",
      "import": "./lib/esm/codec/index.js",
      "require": "./lib/cjs/codec/index.js"
    },
    "./codec/*.js": {
      "types": "./lib/types/codec/*.js",
      "import": "./lib/esm/codec/*.js",
      "require": "./lib/cjs/codec/*.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && tsc -p tsconfig-declarations.json",
    "start": "node lib/index.js",
    "lint": "yarn eslint src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CoderAtParadise/showrunner-time"
  },
  "author": "CoderAtParadise",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CoderAtParadise/showrunner-time/issues"
  },
  "homepage": "https://github.com/CoderAtParadise/showrunner-time#readme",
  "dependencies": {
    "@coderatparadise/showrunner-network": "^1.2.0",
    "@trpc/server": "^10.0.0-alpha.41",
    "uuid": "^8.3.2",
    "zod": "^3.17.10"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.5",
    "@types/node": "^18.6.3",
    "@types/prettier": "^2.6.4",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@yarnpkg/sdks": "^3.0.0-rc.14",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  }
}
