{
    "name": "@coderatparadise/showrunner-time",
    "version": "0.2.0",
    "description": "Time library for showrunner modules",
    "packageManager": "yarn@3.2.1",
    "type": "module",
    "files": [
        "lib/**"
    ],
    "exports": {
        ".": {
            "types": "./lib/types/index.js",
            "import": "./lib/esm/index.js",
            "require": "./lib/cjs/index.js"
        },
        "./*/*.js": {
            "types": "./lib/types/*.js",
            "import": "./lib/esm/*.js",
            "require": "./lib/cjs/*.js"
        },
        "./codec": {
            "types": "./lib/types/codec/index.js",
            "import": "./lib/esm/codec/index.js",
            "require": "./lib/cjs/codec/index.js"
        },
        "./codec/*.js": {
            "types": "./lib/types/codec/*.js",
            "import": "./lib/esm/codec/*.js",
            "require": "./lib/cjs/codec/*.js"
        },
        "./extension": {
            "types": "./lib/types/extension/index.js",
            "import": "./lib/esm/extension/index.js",
            "require": "./lib/cjs/extension/index.js"
        },
        "./extension/**/*.js": {
            "types": "./lib/types/extension/**/*.js",
            "import": "./lib/esm/extension/**/*.js",
            "require": "./lib/cjs/extension/**/*.js"
        }
    },
    "scripts": {
        "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && tsc -p tsconfig-declarations.json",
        "start": "node lib/index.js",
        "lint": "yarn eslint src/**/*.ts"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/CoderAtParadise/showrunner-time"
    },
    "author": "CoderAtParadise",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/CoderAtParadise/showrunner-time/issues"
    },
    "homepage": "https://github.com/CoderAtParadise/showrunner-time#readme",
    "peerDependencies": {
        "@coderatparadise/showrunner-network": "^1.4",
        "@trpc/client": "^10",
        "@trpc/server": "^10",
        "react": "^18",
        "react-dom": "^18",
        "zod": "^3"
    },
    "peerDependenciesMeta": {
        "react": {
            "optional": true
        },
        "react-dom": {
            "optional": true
        }
    },
    "devDependencies": {
        "@coderatparadise/showrunner-network": "^1.4.0",
        "@trpc/client": "^10.0.0-alpha.42",
        "@trpc/server": "^10.0.0-alpha.42",
        "@types/eslint": "^8.4.5",
        "@types/node": "^18.6.5",
        "@types/prettier": "^2.7.0",
        "@types/react": "^18.0.17",
        "@types/react-dom": "^18.0.6",
        "@typescript-eslint/eslint-plugin": "^5.33.0",
        "@typescript-eslint/parser": "^5.33.0",
        "@yarnpkg/sdks": "^3.0.0-rc.14",
        "eslint": "^8.21.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^3.4.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-tsdoc": "^0.2.16",
        "prettier": "^2.7.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "typescript": "^4.7.4",
        "zod": "^3.17.10"
    }
}
