{
  "name": "@coderatparadise/showrunner-time-extension",
  "version": "0.1.0",
  "description": "Extension that adds widgets that support the showrunner-time library",
  "packageManager": "yarn@3.2.1",
  "type": "module",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/cjs/index.d.js",
  "files": [
    "lib/"
  ],
  "exports": {
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/eslint": "^8",
    "@types/prettier": "^2",
    "@types/react": "^18.0.14",
    "eslint": "^8.18.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "prettier": "^2.7.1",
    "showrunner-time": "workspace:^",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "build": "",
    "lint": "yarn eslint src/**/*.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@coderatparadise/showrunner-network": "^1.0.1",
    "@coderatparadise/showrunner-time": "workspace:^",
    "@trpc/client": "^9.26.1",
    "@trpc/server": "^9.26.1"
  }
}
